{
	"compilerOptions": {
		"noImplicitAny": false,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"jsx": "react",
    "lib": [ "dom", "es5", "es6" ],
    "allowSyntheticDefaultImports": true
	},
	"exclude": [
		"node_modules",
		"wwwroot"
	],
  "include": [
    "webpack.config.debug.ts",
    "webpack.config.release.ts"
  ]
}